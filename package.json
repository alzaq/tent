{
  "name": "tent",
  "version": "0.0.1",
  "description": "Superstar admin interface for markdown content with YAML front matter",
  "main": "index.js",
  "dependencies": {
    "ecstatic": "^2.1.0",
    "glob": "^7.1.1",
    "gray-matter": "^2.1.0",
    "lodash": "^4.17.3",
    "markdown-it": "^8.2.2",
    "markdown-it-task-lists": "^1.4.1",
    "routes": "^2.1.0",
    "yo-yo": "^1.3.1"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "coffee-script": "^1.12.2",
    "npm-run-all": "^3.1.2",
    "sheetify": "^6.0.1",
    "sheetify-cssnext": "^1.0.7",
    "toml": "^2.3.1",
    "watchify": "^3.8.0",
    "yo-yoify": "^3.5.0"
  },
  "scripts": {
    "start:client": "watchify -t [ sheetify/transform -u sheetify-cssnext ] -t yo-yoify -t babelify admin.js -o public/bundle.js -vd",
