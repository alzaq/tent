{
  "name": "tent",
  "version": "0.0.5",
  "description": "Superstar admin interface with markdown preview",
  "main": "index.js",
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "fastmatter": "^1.1.1",
    "fs-extra": "^1.0.0",
    "glob": "^7.1.1",
    "hyperapp": "^0.6.0",
    "lodash": "^4.17.3",
    "lodash.debounce": "^4.0.8",
    "markdown-it": "^8.2.2",
    "markdown-it-task-lists": "^1.4.1",
    "yo-yo": "^1.3.1"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "browserify": "^13.3.0",
    "csjs": "^1.0.6",
    "csjs-injectify": "^1.0.1",
    "npm-run-all": "^3.1.2",
    "watchify": "^3.8.0",
    "yo-yoify": "^3.5.0"
  },
  "scripts": {
    "build": "browserify -t csjs-injectify -t yo-yoify -t babelify index.js -o dist/tent.js",
    "server": "node server/index.js",
    "watch:client": "watchify -t csjs-injectify -t yo-yoify -t babelify index.js -o public/bundle.js -vd",
    "watch:server": "cd server && nodemon index.js",
    "watch:dev-server": "browser-sync start --proxy \"localhost:3350\" --port 3351 --files \"public/**\"",
    "start:prod": "npm-run-all build server",
    "start": "npm-run-all --parallel watch:client watch:server watch:dev-server"
  },
  "keywords": [
    "admin",
    "static"
  ],
  "author": "Artur Paikin",
  "license": "MIT"
}
